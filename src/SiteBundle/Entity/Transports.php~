<?php

namespace SiteBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Transports
 */
class Transports
{
    /**
     * @var int
     */
    private $id;

    /**
     * @var string
     */
    private $voiture;

    /**
     * @var string
     */
    private $avion;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set voiture
     *
     * @param string $voiture
     * @return Transports
     */
    public function setVoiture($voiture)
    {
        $this->voiture = $voiture;

        return $this;
    }

    /**
     * Get voiture
     *
     * @return string 
     */
    public function getVoiture()
    {
        return $this->voiture;
    }

    /**
     * Set avion
     *
     * @param string $avion
     * @return Transports
     */
    public function setAvion($avion)
    {
        $this->avion = $avion;

        return $this;
    }

    /**
     * Get avion
     *
     * @return string 
     */
    public function getAvion()
    {
        return $this->avion;
    }
}
